package {{entity.namespace}}.entity.{{lower-snake entity.name}}

data class {{upper-camel entity.name}}Document(
    {{#each entity.stored_properties as |property| ~}}
    {{define 'prop_type' (case
      (eq property.type 'number') 'Int'
      (upper-camel property.type)
    ) ~}}
    val {{lower-camel property.name}}: {{prop_type}},
    {{/each}}
)